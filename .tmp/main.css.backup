ul li>button{font:inherit;width:100%;text-align:left}ul li .pull-in{vertical-align:top}ul+[role=footer],ul+p{margin-top:-25px}



.bg-white {
	background: #ffffff !important;
}
.value {
	color: #777;
	float: right;
	text-align: right;
	margin-left: 8px;
}

.icon_ {
	width: 2em;
	height: 2em;
	border-radius: 20%;
}
.icon__ {
    width: 1.5em;
    height: 1.5em;
    border-radius: 20%;
}
.icon-more + * {
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);
}

ul > li > a > label,
ul > li > p > a > label,
ul > li > button > label,
ul > li > p > button > label {
    margin-left: .5em;
}

ul > li > p {
	overflow: auto;
}

a:focus, a:active {
	text-decoration: none;
}

.icon-more {
    width: 1.5em;
    height: 1.5em;
    display: inline-block;
    line-height: 1;
    border-radius: 20%;
    background-size: 100% 100%;
    background-image: url(data:image/svg+xml;base64,);
}
